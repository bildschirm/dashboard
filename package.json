{
  "name": "mission-control-dashboard",
  "version": "0.0.1",
  "description": "A web UI for HAL6900's Mission Control.",
  "main": "app/js/server-exports.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "npm run development",
    "development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --config=node_modules/laravel-mix/setup/webpack.config.js",
    "watch": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --watch --progress --config=node_modules/laravel-mix/setup/webpack.config.js",
    "start": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --watch --progress --config=node_modules/laravel-mix/setup/webpack.config.js",
    "hot": "cross-env NODE_ENV=development webpack-dev-server --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js",
    "prod": "npm run production",
    "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --progress --config=node_modules/laravel-mix/setup/webpack.config.js",
    "docs": "jsdoc -r app/js -d docs -c jsdoc.config.js"
  },
  "author": "Lukas Mateffy (@Capevace)",
  "license": "MIT",
  "dependencies": {
    "color-convert": "^2.0.0",
    "laravel-mix-polyfill": "^3.0.0",
    "lodash": "^4.17.21",
    "lodash.chunk": "^4.2.0",
    "mission-control-client": "^1.0.0",
    "node-sass": "^4.14.1",
    "portal-vue": "^2.1.7",
    "vue": "^2.6.10",
    "vue-grid-layout": "^2.3.8",
    "vue-observe-visibility": "^0.4.3",
    "vue-router": "^3.5.1",
    "vue-router-multi-view": "^0.1.0",
    "vue-template-compiler": "^2.6.10",
    "vuex": "^3.1.0"
  },
  "devDependencies": {
    "autoprefixer": "^10.2.4",
    "cross-env": "^5.2.0",
    "jsdoc-vuejs": "^2.4.3",
    "laravel-mix": "^6.0.13",
    "parcel-bundler": "^1.12.5",
    "postcss": "^8.2.7",
    "sass": "^1.32.12",
    "sass-loader": "^11.0.1",
    "tailwindcss": "^2.0.3",
    "vue-loader": "^15.9.6",
    "webpack": "^5.23.0"
  }
}
